<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Button?>

<VBox xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.mongraphe.graphui.FenetreGrapheController"
      spacing="10" style="-fx-padding: 20px;">
    
    <TableView fx:id="tableView" prefHeight="300" prefWidth="500"/>
    <HBox alignment="CENTER" spacing="10">
    	<Button text="Continuer" fx:id="continuerButton" onAction="#handleContinuer"/>
    	<Button text="Annuler" fx:id="annulerButton" onAction="#handleAnnuler"/>
	</HBox>

    
</VBox>
